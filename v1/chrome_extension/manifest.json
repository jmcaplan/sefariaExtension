        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/sefaria_logo16.png",
            "32": "images/sefaria_logo32.png",
            "48": "images/sefaria_logo48.png",
            "128": "images/sefaria_logo128.png"
        }
    },
    "icons": {
        "16": "images/sefaria_logo16.png",
        "32": "images/sefaria_logo32.png",
        "48": "images/sefaria_logo48.png",
        "128": "images/sefaria_logo128.png"
    },
    "manifest_version": 2
}