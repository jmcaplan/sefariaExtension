{
    "name": "Jastrow Jackpot",
    "version": "1.0",
    "description": "Project for Sefaria Hackathon 2020",
    "permissions": [
        "activeTab",
        "declarativeContent",
        "tabs",
        "storage",
        "https://lpn3v4dlk3.execute-api.us-east-1.amazonaws.com/*"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
